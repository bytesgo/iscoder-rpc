<?xml version="1.0"?>
<configuration>

<!-- service name -->
<property>
<name>scf.service.name</name>
<value>demo</value>
</property>

<!-- socket server is enable -->
<property>
<name>scf.server.http.enable</name>
<value>true</value>
</property>

<property>
<name>scf.server.tcp.listenPort</name>
<value>10000</value>
</property>

<!-- socket server listent ip -->
<property>
<name>scf.server.tcp.listenIP</name>
<value>192.168.1.123</value>
</property>

<!-- global reqeust filter -->
<property>
<name>scf.init</name>
<value append="true">com.bj58.zhaopin.scf.pusher.zpt.components.Global</value>
</property>

</configuration>